{
  "name": "material-custom-elements",
  "version": "1.0.1",
  "description": "Material Design implemented in Custom elements",
  "main": "src/bundle.js",
  "scripts": {
    "start": "ws -p 8080 --spa index.html",
    "start:bundle": "webpack-dev-server --config webpack.config.bundle.js",
    "start:legacy": "webpack-dev-server --config webpack.config.legacy.js",
    "test": "ws -p 8080 --spa test/index.html",
    "test:headless": "concurrently \"npm test\" \"sleep 1 && mocha-headless-chrome -f http://localhost:8080/\"",
    "clean:dist": "rm -rf dist/*",
    "webpack": "webpack --config webpack.config.common.js",
    "webpack:bundle": "webpack --config webpack.config.bundle.js",
    "webpack:legacy": "webpack --config webpack.config.legacy.js",
    "prebuild": "npm run clean:dist",
    "prebuild:bundle": "npm run clean:dist",
    "build": "npm run webpack",
    "build:bundle": "npm run webpack:bundle",
    "build:legacy": "npm run webpack:legacy",
    "reinstall": "rm -rf node_modules && npm install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Danny Moerkerke <danny@dannymoerkerke.nl>",
  "license": "ISC",
  "devDependencies": {
    "@storybook/cli": "^4.0.0-alpha.8",
    "@storybook/html": "^4.0.0-alpha.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "concurrently": "^4.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "local-web-server": "^2.2.4",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "sinon": "^7.0.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^1.0.17",
    "autoprefixer": "^8.3.0",
    "css-loader": "^0.28.11",
    "custom-elements-css-loader": "git@bitbucket.org:dannymoerkerke/custom-elements-css-loader.git",
    "fetch-polyfill": "^0.8.2",
    "mocha": "^5.2.0",
    "mocha-headless-chrome": "^2.0.0",
    "postcss": "^6.0.21",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.4",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
}
